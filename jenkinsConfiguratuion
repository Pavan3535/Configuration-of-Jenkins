1  apt-get -y
    2  apt-get update -y
    3  apt install openjdk-ll-jdk -y
    4  apt install openjdk-ll-jdk
    5  sudo apt install default-jdk
    6  java --version
    7  curl -fsSL https://pkg.jenkins.io/debian/jenkins.io-2023.key | sudo tee \/usr/share/keyrings/jenkins-keyring.asc > /dev/null
    8  echo deb [signed-by=/usr/share/keyrings/jenkins-keyring.asc] \https://pkg.jenkins.io/debian binary/ | sudo tee \/etc/apt/sources.list.d/jenkins.list > /dev/null
    9  sudo apt-get update
   10  sudo apt-get install jenkins
   11  which jenkins
   12  systemctl status jenkins
